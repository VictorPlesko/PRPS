<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="720.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.UsersController">
   <children>
      <AnchorPane prefHeight="64.0" prefWidth="1029.0" style="-fx-background-color: #FAAC58;">
         <children>
            <Button fx:id="logOutButton" layoutX="875.0" layoutY="11.0" mnemonicParsing="false" style="-fx-border-radius: 20; -fx-background-radius: 20;" text="Log out">
               <font>
                  <Font name="Comic Sans MS Bold Italic" size="20.0" />
               </font>
            </Button>
            <Label layoutX="14.0" layoutY="11.0" text="Testing System">
               <font>
                  <Font name="Comic Sans MS Bold Italic" size="25.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <TableView fx:id="tableView" layoutY="64.0" prefHeight="636.0" prefWidth="1029.0">
        <columns>
          <TableColumn fx:id="loginColumn" prefWidth="170.0" text="Login" />
          <TableColumn fx:id="passwordColumn" prefWidth="165.0" text="Password" />
            <TableColumn fx:id="typeColumn" prefWidth="182.0" text="Type" />
            <TableColumn fx:id="nameColumn" prefWidth="176.0" text="Name" />
            <TableColumn fx:id="surnameColumn" prefWidth="192.0" text="Surname" />
            <TableColumn fx:id="patronymiColumn" prefWidth="143.0" text="Patronymi" />
        </columns>
      </TableView>
      <AnchorPane layoutY="699.0" prefHeight="77.0" prefWidth="1029.0" style="-fx-background-color: #FAAC58;">
         <children>
            <Button fx:id="createUserButton" layoutX="571.0" layoutY="14.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="411.0" style="-fx-border-radius: 20; -fx-background-radius: 20;" text="Create user">
               <font>
                  <Font name="Comic Sans MS Bold Italic" size="20.0" />
               </font>
            </Button>
            <Button fx:id="deleteUserButton" layoutX="83.0" layoutY="14.0" mnemonicParsing="false" onAction="#deletePerson" prefHeight="42.0" prefWidth="411.0" style="-fx-border-radius: 20; -fx-background-radius: 20;" text="Delete user">
               <font>
                  <Font name="Comic Sans MS Bold Italic" size="20.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
